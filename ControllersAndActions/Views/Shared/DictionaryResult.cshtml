
@model Dictionary<string,string>

<div class="m-1 p-1">
    <table class="table table-bordered table-striped table-sm">
        <tr>
            <th>Name</th>
            <th>Value</th>

        </tr>
        @foreach (string key in Model.Keys)
        { <tr><td>@key</td><td>@Model[key]asp-route-id="hello"p</td></tr>}
    </table>
</div>
